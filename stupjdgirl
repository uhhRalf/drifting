game.Workspace.VehiclesFolder[tostring(getPlayer(TextBox_2.Text))].DescendantAdded:Connect(function(descendant)
    wait(1)
    if scriptEnabled == true then
        if DriveMode == "Drift" then
    driftfunc() 
        elseif DriveMode == "Race" then
            racefunc()
        else return end
    else return end
 end)
